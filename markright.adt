CommandChar: "@"
Delimiters: "(){}[]<>,"

BaseCommand<T>: {
  name: string,       // sin delimitadores
  args: List<string>, // sin delimitadores
  children: List<T>
}

Markright: List<Item>
Item: { rawContent: string } * (BlockCommand | Line)
  BlockCommand: BaseCommand<Item>
  Line: { children: List<InlineItem> }
    InlineItem: InlineCommand | Text
      InlineCommand: BaseCommand<InlineItem>
      Text: { child: string }
